{
  "name": "aws-cdk-dynamodb-seeder-examples",
  "version": "1.44.0",
  "description": "Example usage of aws-cdk-dynamodb-seeder",
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "check": "npm test && npm run lint",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elegantdevelopment/aws-cdk-dynamodb-seeder-examples.git"
  },
  "keywords": [
    "aws-cdk-dynamodb-seeder",
    "examples",
    "aws",
    "cdk",
    "dynamodb",
    "seed",
    "seeder"
  ],
  "author": "Justin Taylor <jtaylor@elegantdevelopment.co.uk> (https://www.linkedin.com/in/jsdtaylor)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/elegantdevelopment/aws-cdk-dynamodb-seeder-examples/issues"
  },
  "homepage": "https://github.com/elegantdevelopment/aws-cdk-dynamodb-seeder-examples#readme",
  "dependencies": {
    "@aws-cdk/aws-dynamodb": "^1.44.0",
    "@aws-cdk/core": "^1.44.0",
    "aws-cdk-dynamodb-seeder": "^1.44.0",
    "jest": "^25.5.4",
    "minimist": ">=1.2.6"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.44.0",
    "@types/jest": "^25.2.3",
    "aws-cdk": "^1.44.0",
    "prettier": "^2.0.5",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "@aws-cdk/aws-dynamodb": "^1.44.0",
    "@aws-cdk/core": "^1.44.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.ts",
      "!node_modules/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      },
      "testEnvironment": "node"
    }
  }
}
